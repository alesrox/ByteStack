// TODO: filter and map functions

func multiply_by_2(int item) -> int {
    return item * 2; 
}

func is_even(int item) -> bool {
    return item % 2 == 0; 
}

func is_odd(int item) -> bool {
    return not is_even(item);
}

int[] x = [1, 2, 3, 4];
print(x.map(multiply_by_2));
